    - 9.9.9.9
    - 114.114.114.114
  nameserver: #!replace
    - https://doh.pub/dns-query
    - https://dns.alidns.com/dns-query
  fake-ip-filter: #!replace
    - '*.lan'
    - '*.internal'
    - '*.labproof.internal'
    - '*.peplink.com'
    - '*.smartone.com'
    - home-opnsense.ddns.net
    - homewanip.ddns.net
